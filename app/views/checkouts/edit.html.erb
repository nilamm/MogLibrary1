<%= @checkout.user.name_and_username %>
<%= @checkout.resource.title %>
		<%= form_for @checkout, url: {action: "update"} do |f| %>
			<div class="form-group">
				<%= f.label :notes, class: 'col-sm-3 control-label' %>
				<div class="col-sm-9">
				<%= f.text_field :notes, class: 'form-control form_target'%>
				</div>
			</div>
			<div class="subm_edit_btn">
				<%= f.submit "Update", class: 'btn-info active'%>
			</div>
		<% end %>
